class MyShape:
    def __init__(self, color="Blue", is_filled=False):
        self.color = color
        self.is_filled = is_filled

    def display_info(self):
        print(f"Color: {self.color}")
        print(f"Is Filled: {self.is_filled}")


if __name__ == "__main__":

    default_shape = MyShape()


    default_shape.display_info()

    
    custom_shape = MyShape(color="Green", is_filled=True)

    
    custom_shape.display_info()
