#1
import re

txt = "The rain in Spain"
x = re.search("ai", txt)
print(x) #this will print an object

#2
import re

txt = "The rain in Spain"
x = re.search(r"\bS\w+", txt)
print(x.span())

#3
import re

txt = "The rain in Spain"
x = re.search(r"\bS\w+", txt)
print(x.string)

#4
import re

txt = "The rain in Spain"
x = re.search(r"\bS\w+", txt)
print(x.group())
